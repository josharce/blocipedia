<h1>All Wikis</h1>

<div> 
  <% @wikis.each do |wiki| %>
    <div class="media">
      <div class="media-body">
        <h4 class="media-heading">
          <p><%= link_to wiki.title, wiki %></p>
        </h4>
      </div>
    </div>
  <% end %>
</div>

<div>
  <%= link_to "New Wiki", new_wiki_path(@wiki), class: 'btn btn-success' %>
</div>
